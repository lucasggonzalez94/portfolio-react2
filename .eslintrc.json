{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-tabs":"error",
        "no-console": "warn",
        "no-debugger": "warn",
        "no-trailing-spaces": "error",
        "comma-dangle": ["error", "never"],
        "no-unsafe-optional-chaining": "off",
        "no-nonoctal-decimal-escape": "off"
    }
}
